{
    "manifest_version": 3,
    "name": "Acelerador de descargas by Edouard",
    "version": "1.2",
    "permissions": ["downloads","storage", "cookies"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
      },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "descargas16x16.png",
        "32": "descargas32x32.png",
        "64": "descargas64x64.png"
      }
    },
    "icons": {
      "16": "descargas16x16.png",
      "32": "descargas32x32.png",
      "64": "descargas64x64.png"
    }
}